{
  "compilerOptions": {
    "declaration": true,
    "lib": ["dom", "es6"],
    "noEmitOnError": true,
    "strict": true,
    "noImplicitAny": false,
    "noUnusedLocals": true,
    "module": "commonjs",
    "moduleResolution": "node",
    "target": "ES6",
    "outDir": "../lib",
    "skipLibCheck": true,
    "sourceMap": true
  }
}
